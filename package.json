{
  "name": "project_eurogranite",
  "version": "0.1.1",
  "private": true,
  "homepage": "https://Gennadiy01.github.io/eurogranite-website",
  "adminHomepage": "https://Gennadiy01.github.io/eurogranite-website",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "lucide-react": "^0.541.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.92.1",
    "web-vitals": "^2.1.4",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "cross-env PUBLIC_URL= react-scripts start",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "react-scripts build && node scripts/generate-static-pages.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:admin": "gh-pages -d build -b gh-pages-admin",
    "dev": "cross-env PORT=3001 PUBLIC_URL= react-scripts start",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "analyze": "npm run build:clean && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:clean": "react-scripts build",
    "perf:analyze": "npm run build:clean && npx webpack-bundle-analyzer build/static/js/*.js",
    "ci:bundle-check": "npm run build:clean && npx webpack-bundle-analyzer build/static/js/*.js --no-open",
    "bundle:size": "npm run build:clean && node scripts/bundle-size-check.js",
    "bundle:check": "node scripts/bundle-size-check.js",
    "bundle:compare": "node scripts/bundle-compare.js compare",
    "bundle:save": "node scripts/bundle-compare.js save",
    "build:local": "npm run prebuild && set PUBLIC_URL=.&& set GENERATE_SOURCEMAP=false&& react-scripts build && node scripts/generate-local-pages.js",
    "test:regression": "node scripts/regression-test.js",
    "test:refactoring": "npm run test:regression"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
