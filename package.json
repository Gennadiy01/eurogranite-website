{
  "name": "project_eurogranite",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://Gennadiy01.github.io/eurogranite-website",
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.22.0",
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cloudinary": "^2.7.0",
    "cloudinary-react": "^1.8.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.541.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.8.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.92.1",
    "web-vitals": "^2.1.4",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "react-scripts build && react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "set PORT=3005 && react-scripts start",
    "generate-sitemap": "node scripts/generate-sitemap.js"
  },
  "reactSnap": {
    "inlineCss": false,
    "minifyHtml": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "include": [
      "/",
      "/ua",
      "/ua/products",
      "/ua/about",
      "/ua/contact",
      "/ua/gallery",
      "/ua/articles",
      "/en",
      "/en/products",
      "/en/about",
      "/en/contact",
      "/en/gallery",
      "/en/articles",
      "/de",
      "/de/products",
      "/de/about",
      "/de/contact",
      "/de/gallery",
      "/de/articles",
      "/pl",
      "/pl/products",
      "/pl/about",
      "/pl/contact",
      "/pl/gallery",
      "/pl/articles"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0"
  }
}
